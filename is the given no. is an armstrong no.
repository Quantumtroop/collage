#include<stdio.h>
int main()
{
    int a, b, c, sum;
    printf("enter the number you want to check \n");
    scanf("%d", &a);
    sum=0;
    c=a;
    while(a!=0)
    {
        b=a%10;
        sum=sum+b*b*b;
        a=a/10;
    }
    if(c==sum)
    {
        printf("the number is ast.....");
    }
    else
    {
        printf("Ji nahi Hai");
    }
    
    
    return 0;
}
